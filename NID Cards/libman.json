{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap-filestyle@2.1.0",
      "destination": "wwwroot/bootstrap-filestyle/"
    },
    {
      "library": "bootstrap@5.2.0",
      "destination": "wwwroot/bootstrap/"
    },
    {
      "library": "bootstrap-datepicker@1.9.0",
      "destination": "wwwroot/lib/bootstrap-datepicker/",
      "files": [
        "js/bootstrap-datepicker.min.js",
        "css/bootstrap-datepicker.min.css"
      ]
    },
    {
      "library": "bootbox.js@5.5.3",
      "destination": "wwwroot/lib/bootbox.js/"
    }
  ]
}